<?import javafx.stage.Stage?>

<Stage xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainAppController">
    <scene>
        <Scene>
            <AnchorPane prefHeight="662.0" prefWidth="874.0">
            <children>
               <ListView fx:id="albumlistview" layoutX="280.0" layoutY="447.0" prefHeight="188.0" prefWidth="577.0" />
               <ListView fx:id="albuminfo_listview" layoutX="16.0" layoutY="444.0" prefHeight="194.0" prefWidth="247.0" />
               <ListView fx:id="taglistview" layoutX="14.0" layoutY="209.0" prefHeight="205.0" prefWidth="247.0" />
               <Button fx:id="editcaptionbutton" layoutX="30.0" layoutY="14.0" mnemonicParsing="false" onAction="#buttonPress" prefHeight="27.0" prefWidth="100.0" text="Edit Caption" />
               <Button fx:id="deletetagbutton" layoutX="30.0" layoutY="55.0" mnemonicParsing="false" onAction="#buttonPress" prefHeight="27.0" prefWidth="100.0" text="Delete Tag" />
               <Button fx:id="addtagbutton" layoutX="161.0" layoutY="55.0" mnemonicParsing="false" onAction="#buttonPress" prefHeight="27.0" prefWidth="113.0" text="Add Tag" />
               <Button fx:id="addphotobutton" layoutX="301.0" layoutY="14.0" mnemonicParsing="false" onAction="#buttonPress" prefHeight="27.0" prefWidth="113.0" text="Add Photo" />
