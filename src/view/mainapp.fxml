               <Button fx:id="searchbutton" layoutX="733.0" layoutY="14.0" mnemonicParsing="false" onAction="#buttonPress" prefHeight="27.0" prefWidth="108.0" text="Search" />
               <Button fx:id="logoutbutton" layoutX="733.0" layoutY="55.0" mnemonicParsing="false" onAction="#buttonPress" prefHeight="27.0" prefWidth="109.0" text="Logout" />
               <Button fx:id="copymovebutton" layoutX="574.0" layoutY="14.0" mnemonicParsing="false" onAction="#buttonPress" prefHeight="27.0" prefWidth="132.0" text="Copy/Move Photos" />
