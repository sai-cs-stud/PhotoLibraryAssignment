               <Label layoutX="516.0" layoutY="427.0" text="Album List" />
               <Label layoutX="103.0" layoutY="427.0" text="Album Info" />
               <Label layoutX="108.0" layoutY="186.0" text="Tag List" />
               <Label layoutX="66.0" layoutY="97.0" prefHeight="17.0" prefWidth="132.0" text="Selected Photo Caption" />
               <TextArea fx:id="photocaption" layoutX="16.0" layoutY="114.0" prefHeight="66.0" prefWidth="247.0" />
               <TextField fx:id="newtaginput" layoutX="159.0" layoutY="14.0" prefHeight="27.0" prefWidth="117.0" promptText="{person, sesh}" />
               <ScrollPane fx:id="myscrollpane" layoutX="280.0" layoutY="109.0" prefHeight="313.0" prefWidth="580.0">
                  <content>
                     <TilePane fx:id="mytilepane" prefHeight="315.0" prefWidth="580.0" />
                  </content>
               </ScrollPane>
            </children></AnchorPane>
        </Scene>
    </scene>
</Stage>
