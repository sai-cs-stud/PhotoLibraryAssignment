<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.stage.Stage?>

<Stage xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.AdminController">
    <scene>
        <Scene>
            <AnchorPane prefHeight="414.0" prefWidth="528.0">
            <children>
               <TextField fx:id="userinputbar" layoutX="28.0" layoutY="14.0" prefHeight="27.0" prefWidth="191.0" promptText="user to be created" />
               <Button fx:id="createuserbutton" layoutX="238.0" layoutY="14.0" mnemonicParsing="false" onAction="#buttonPress" text="Create User" />
               <Button fx:id="deleteuserbutton" layoutX="340.0" layoutY="14.0" mnemonicParsing="false" onAction="#buttonPress" text="Delete user" />
               <Button fx:id="logout" layoutX="439.0" layoutY="14.0" mnemonicParsing="false" onAction="#buttonPress" prefHeight="27.0" prefWidth="68.0" text="Logout" />
               <ListView fx:id="listview" layoutX="24.0" layoutY="52.0" prefHeight="341.0" prefWidth="473.0" />
               <ScrollBar fx:id="scrollbar" layoutX="498.0" layoutY="53.0" orientation="VERTICAL" prefHeight="342.0" prefWidth="17.0" />
            </children></AnchorPane>
        </Scene>
    </scene>
</Stage>
